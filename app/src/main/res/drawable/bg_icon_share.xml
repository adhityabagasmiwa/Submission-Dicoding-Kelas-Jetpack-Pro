<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/colorSecondary" />
            <size
                android:width="48dp"
                android:height="48dp" />
            <padding
                android:bottom="@dimen/dimen_extra_small"
                android:left="@dimen/dimen_extra_small"
                android:right="@dimen/dimen_extra_small"
                android:top="@dimen/dimen_extra_small" />
        </shape>
    </item>
    <item
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/ic_share"
        android:gravity="center"
        android:right="3dp" />
</layer-list>